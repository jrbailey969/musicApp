  },
  "dependencies": {
    "angular": "^1.6",
    "angular-ui-router": "^1.0.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.5",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-loader": "1.9.0",
