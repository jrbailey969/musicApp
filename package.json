    "ng-cache-loader": "^0.0.25",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "require-dir": "^0.3.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
