    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-config": "^7.0.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-stream": "^3.2.0",
    "yargs": "^8.0.2"
  },
  "scripts": {
    "pretest": "webdriver-manager update",
    "test": "karma start",
    "test:e2e": "protractor ./test/protractor.conf.js",
